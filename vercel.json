{
    "routes": [
        {
            "src": "/(.*\\.css)",
            "headers": { "Content-Type": "text/css" },
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.js)",
            "headers": { "Content-Type": "application/javascript" },
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.svg)",
            "headers": { "Content-Type": "image/svg+xml" },
            "dest": "/$1"
        },
        {
            "src": "/freemusic/(.*)",
            "dest": "/freemusic/$1"
        },
        {
            "src": "/freemusic\\.json",
            "dest": "/freemusic.json"
        },
        {
            "src": "/songs\\.json",
            "dest": "/songs.json"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
