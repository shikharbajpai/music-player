{
  "version": 2,
  "name": "music-player",
  "public": true,
  "routes": [
    { "source": "/(.*)", "destination": "/index.html" },
    { "src": "/songs/(.*)", "dest": "/songs/$1" },
    { "src": "/songs/freemusic/(.*)", "dest": "/songs/freemusic/$1" },
    { "src": "/songs/freemusic/Hazy After Hours.mp3", "dest": "/songs/freemusic/Hazy%20After%20Hours.mp3" }
  ]
}
